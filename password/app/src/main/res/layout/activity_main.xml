<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">
    <include android:id="@+id/e75132465435" layout="@layout/default_toolbar" />

    <SearchView
        android:id="@+id/main_search_input"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:queryHint="搜索"
        app:layout_constraintTop_toBottomOf="@+id/e75132465435" />
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview_keyList"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintVertical_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_search_input"/>

</androidx.constraintlayout.widget.ConstraintLayout>